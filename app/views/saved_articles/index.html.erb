<p id="notice"><%= notice %></p>

<h1>Saved Articles</h1>

<table>
  <thead>
    <tr>
      <th>Article</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_articles.each do |saved_article| %>
      <tr>
        <td><%= saved_article.article_id %></td>
        <td><%= saved_article.user_id %></td>
        <td><%= link_to 'Show', saved_article %></td>
        <td><%= link_to 'Edit', edit_saved_article_path(saved_article) %></td>
        <td><%= link_to 'Destroy', saved_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved Article', new_saved_article_path %>
